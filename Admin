-- oque est√° fazendo aqui? N√£o adianta executar, voc√™ n√£o vai ganhar poderes kkkkkkkkkk
-- XXMZ | Admin Hub - Server Joiner
-- Carrega a biblioteca Fluent
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")

-- Cria a janela
local Window = Fluent:CreateWindow({
    Title = "XXMZ | Admin Hub",
    SubTitle = "Join server",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

-- Cria as abas
local Tabs = {
    Main = Window:AddTab({ Title = "Server Joiner", Icon = "server" }),
    Info = Window:AddTab({ Title = "Informa√ß√µes", Icon = "info" })
}

-- ========================================
-- ABA PRINCIPAL - SERVER JOINER
-- ========================================

local jobIdInput = ""

Tabs.Main:AddParagraph({
    Title = "üåê Teleporte para Servidor",
    Content = "Cole o Job ID do servidor que deseja entrar e clique em teleportar."
})

-- Input para o Job ID
Tabs.Main:AddInput("JobIdInput", {
    Title = "Job ID do Servidor",
    Default = "",
    Placeholder = "Cole o Job ID aqui...",
    Numeric = false,
    Finished = true,
    Callback = function(value)
        jobIdInput = value
    end
})

-- Bot√£o para copiar Job ID atual
Tabs.Main:AddButton({
    Title = "üìã Copiar Job ID Atual",
    Description = "Copia o Job ID do servidor atual",
    Callback = function()
        local currentJobId = game.JobId
        setclipboard(currentJobId)
        Fluent:Notify({
            Title = "Job ID Copiado!",
            Content = "Job ID atual copiado para √°rea de transfer√™ncia",
            Duration = 3
        })
    end
})

-- Bot√£o para teleportar
Tabs.Main:AddButton({
    Title = "üöÄ Teleportar para Servidor",
    Description = "Entra no servidor com o Job ID especificado",
    Callback = function()
        if jobIdInput == "" or jobIdInput == nil then
            Fluent:Notify({
                Title = "Erro!",
                Content = "Por favor, insira um Job ID v√°lido",
                Duration = 5
            })
            return
        end
        
        Fluent:Notify({
            Title = "Teleportando...",
            Content = "Entrando no servidor especificado",
            Duration = 3
        })
        
        local success, err = pcall(function()
            TeleportService:TeleportToPlaceInstance(game.PlaceId, jobIdInput, Players.LocalPlayer)
        end)
        
        if not success then
            Fluent:Notify({
                Title = "Erro ao Teleportar!",
                Content = "Job ID inv√°lido ou servidor n√£o encontrado",
                Duration = 5
            })
        end
    end
})

Tabs.Main:AddSection("Servidores Recentes")

-- Bot√£o para servidor aleat√≥rio
Tabs.Main:AddButton({
    Title = "üé≤ Servidor Aleat√≥rio",
    Description = "Entra em um servidor aleat√≥rio do jogo",
    Callback = function()
        Fluent:Notify({
            Title = "Teleportando...",
            Content = "Procurando servidor aleat√≥rio",
            Duration = 3
        })
        
        TeleportService:Teleport(game.PlaceId, Players.LocalPlayer)
    end
})

-- ========================================
-- ABA DE INFORMA√á√ïES
-- ========================================

Tabs.Info:AddParagraph({
    Title = "‚ÑπÔ∏è Informa√ß√µes do Servidor Atual",
    Content = string.format(
        "üéÆ Jogo: %s\n" ..
        "üÜî Place ID: %d\n" ..
        "üåê Job ID: %s\n" ..
        "üë• Jogadores: %d/%d",
        game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,
        game.PlaceId,
        game.JobId,
        #Players:GetPlayers(),
        Players.MaxPlayers
    )
})

Tabs.Info:AddParagraph({
    Title = "üìñ Como Usar",
    Content = 
        "1. Para copiar o Job ID do servidor atual, clique em 'Copiar Job ID Atual'\n\n" ..
        "2. Para entrar em um servidor espec√≠fico:\n" ..
        "   ‚Ä¢ Cole o Job ID no campo de texto\n" ..
        "   ‚Ä¢ Clique em 'Teleportar para Servidor'\n\n" ..
        "3. Use 'Servidor Aleat√≥rio' para entrar em qualquer servidor dispon√≠vel"
})

Tabs.Info:AddParagraph({
    Title = "‚ö†Ô∏è Avisos",
    Content = 
        "‚Ä¢ O Job ID √© √∫nico para cada servidor\n" ..
        "‚Ä¢ Servidores podem estar cheios ou offline\n" ..
        "‚Ä¢ Alguns jogos podem ter restri√ß√µes de teleporte"
})

-- ========================================
-- NOTIFICA√á√ÉO INICIAL
-- ========================================

Fluent:Notify({
    Title = "XXMZ | Admin Hub",
    Content = "Hub carregado com sucesso!",
    Duration = 5
})

print("‚úÖ XXMZ | Admin Hub carregado com sucesso!")
