-- =============================
-- TELEPORTS TAB
-- =============================
local TeleportsTab = Window:Tab({
    Title = "Teleports",
    Icon = "map-pin",
    Locked = false,
})

local function teleportTo(x, y, z)
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(x, y, z))
    end
end

TeleportsTab:Button({
    Title = "Normal Arena",
    Desc = "Teleportar para Arena Normal",
    Locked = false,
    Callback = function()
        teleportTo(-14, 66, -1)
    end
})

TeleportsTab:Button({
    Title = "Slapple Island",
    Desc = "Teleportar para Ilha Slapple",
    Locked = false,
    Callback = function()
        teleportTo(-395, 51, -13)
    end
})

TeleportsTab:Button({
    Title = "Cannon Island",
    Desc = "Teleportar para Ilha do Canhão",
    Locked = false,
    Callback = function()
        teleportTo(263, 33, 197)
    end
})

