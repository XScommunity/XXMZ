-- =============================
-- COMBAT TAB
-- =============================
local CombatTab = Window:Tab({
    Title = "Combat",
    Icon = "sword",
    Locked = false,
})

local Keybind = CombatTab:Keybind({
    Title = "Anti Cooldown Ability",
    Desc = "dont spam, or u will get kicked",
    Value = "Z",
    Callback = function(v)
        local UserInputService = game:GetService("UserInputService")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        
        UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if gameProcessed then return end
            if input.KeyCode == Enum.KeyCode[v] then
                -- Ativa 1 vez
                ReplicatedStorage:WaitForChild("GeneralAbility"):FireServer()
                
                WindUI:Notify({
                    Title = "Ability",
                    Content = "Habilidade ativada!",
                    Duration = 1,
                    Icon = "zap",
                })
            end
        end)
        
        WindUI:Notify({
            Title = "Anti Cooldown Ability",
            Content = "Keybind configurada para: " .. v,
            Duration = 2,
            Icon = "keyboard",
        })
    end
})

local slapAuraStopper
CombatTab:Toggle({
    Title = "Slap Aura",
    Desc = "Ativa aura de slap automático",
    Icon = "zap",
    Type = "Checkbox",
    Default = false,
    Callback = function(state)
        if state then
            slapAuraStopper = loadstring(game:HttpGet("https://raw.githubusercontent.com/TheWExploiter/Slap-Battles-TP/refs/heads/main/slapaura.lua"))()
            WindUI:Notify({
                Title = "Slap Aura",
                Content = "Slap Aura ativado!",
                Duration = 3,
                Icon = "check",
            })
        else
            if slapAuraStopper then
                slapAuraStopper()
            end
            WindUI:Notify({
                Title = "Slap Aura",
                Content = "Slap Aura desativado!",
                Duration = 2,
                Icon = "x",
            })
        end
    end
})

local antiVoidPart = Instance.new("Part")
antiVoidPart.Size = Vector3.new(100000000, 3, 100000000)
antiVoidPart.Position = Vector3.new(0, -11.5, 0)
antiVoidPart.Anchored = true
antiVoidPart.Color = Color3.fromRGB(255, 255, 255)
antiVoidPart.CanCollide = true
antiVoidPart.Transparency = 0.7
antiVoidPart.Parent = game.Workspace

CombatTab:Toggle({
    Title = "Anti Void",
    Desc = "Previne quedas no void",
    Icon = "shield",
    Type = "Checkbox",
    Default = true,
    Callback = function(state)
        if state then
            antiVoidPart.Transparency = 0.7
            antiVoidPart.CanCollide = true
        else
            antiVoidPart.Transparency = 1
            antiVoidPart.CanCollide = false
        end
    end
})

CombatTab:Toggle({
    Title = "Anti Ragdoll",
    Desc = "Previne ragdoll (precisa resetar)",
    Icon = "user-check",
    Type = "Checkbox",
    Default = false,
    Callback = function(state)
        if state then
            player.CharacterAdded:Connect(function(character)
                local humanoid = character:WaitForChild("Humanoid")
                local ragdoll = character:WaitForChild("Ragdolled")

                ragdoll.Changed:Connect(function()
                    if ragdoll.Value then
                        local hrp = character:FindFirstChild("HumanoidRootPart")
                        if hrp then
                            hrp.Anchored = true
                            wait(2)
                            hrp.Anchored = false
                        end
                    end
                end)
            end)
            WindUI:Notify({
                Title = "Anti Ragdoll",
                Content = "Anti Ragdoll ativado! Resete para funcionar.",
                Duration = 3,
                Icon = "check",
            })
        end
    end
})

CombatTab:Input({
    Title = "WalkSpeed",
    Desc = "Alterar velocidade de movimento",
    Placeholder = "16",
    Callback = function(val)
        local speed = tonumber(val)
        if speed and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = speed
            WindUI:Notify({
                Title = "WalkSpeed",
                Content = "Velocidade alterada para " .. speed,
                Duration = 3,
                Icon = "check",
            })
        end
    end
})

